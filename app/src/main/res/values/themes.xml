<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.RecycloWaste" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/green_new</item>
        <item name="colorPrimaryVariant">@color/green_new</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/green_new</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/icon_color</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="DialogStyle" parent="Theme.AppCompat.DayNight.Dialog.Alert">
        <item name="windowMinWidthMajor">100%</item>
        <item name="windowMinWidthMinor">100%</item>
        <item name="colorPrimary">@color/green_new</item>
        <item name="colorPrimaryVariant">@color/green_new</item>
        <item name="colorSecondary">@color/green_new</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
    </style>

</resources>